{
  "name": "@cumulus/aws-client",
  "version": "1.21.0",
  "description": "Utilities for working with AWS",
  "keywords": [
    "GIBS",
    "CUMULUS",
    "NASA"
  ],
  "engines": {
    "node": ">=10.16.3"
  },
  "files": [
    "./*.js",
    "./*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/nasa/cumulus/tree/master/packages/aws-client/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/nasa/cumulus"
  },
  "scripts": {
    "build-docs": "npm run tsc && jsdoc2md -t README.hbs *.js > README.md",
    "lint": "eslint .",
    "prepare": "npm run tsc",
    "test": "ava",
    "test-coverage": "nyc ava",
    "test-ci": "npm run lint",
    "tsc": "tsc"
  },
  "ava": {
    "files": [
      "tests/**"
    ],
    "verbose": true
  },
  "nyc": {
    "exclude": [
      "tests",
      "test"
    ]
  },
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "dependencies": {
    "@cumulus/checksum": "1.21.0",
    "@cumulus/errors": "1.21.0",
    "@cumulus/json-logger": "1.21.0",
    "aws-sdk": "^2.585.0",
    "jsonpath-plus": "^1.1.0",
    "lodash": "~4.17.15",
    "p-map": "^1.2.0",
    "p-retry": "^4.2.0",
    "pump": "^3.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.150",
    "@types/pump": "^1.1.0",
    "ava": "^2.1.0",
    "crypto-random-string": "^3.2.0",
    "delay": "^4.3.0",
    "jsdoc-to-markdown": "^5.0.3",
    "nyc": "^14.0.0",
    "p-timeout": "^3.2.0",
    "sinon": "^8.0.4",
    "typescript": "^3.8.3"
  }
}
