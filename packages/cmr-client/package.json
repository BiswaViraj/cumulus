{
  "name": "@cumulus/cmr-client",
  "version": "1.21.0",
  "engines": {
    "node": ">=10.16.3"
  },
  "scripts": {
    "build-docs": "jsdoc2md --heading-depth 2 --template templates/API.hbs CMR.js CMRSearchConceptQueue.js > API.md",
    "test": "ava",
    "test-coverage": "nyc ava",
    "debug": "NODE_ENV=test node --inspect-brk node_modules/ava/profile.js --serial tests/*.js"
  },
  "ava": {
    "fail-fast": true
  },
  "nyc": {
    "exclude": [
      "tests"
    ]
  },
  "keywords": [
    "CUMULUS"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nasa/cumulus"
  },
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "dependencies": {
    "@cumulus/json-logger": "1.21.0",
    "got": "^9.6.0",
    "lodash": "^4.17.15",
    "public-ip": "^3.0.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "ava": "^2.1.0",
    "jsdoc-to-markdown": "^4.0.1",
    "nock": "^10.0.6",
    "sinon": "^7.1.1"
  }
}
